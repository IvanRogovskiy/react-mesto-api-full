{ 
    "env": { 
        "browser": true, 
        "commonjs": true, 
        "es2021": true 
    }, 
    "extends": [ 
        "airbnb-base" 
    ], 
    "parserOptions": { 
        "ecmaVersion": "latest" 
    }, 
    "rules": { 
      "consistent-return": "off", 
      "no-underscore-dangle":  ["error", { "allow": ["_id"] }], 
      "new-cap": ["error", { "newIsCap": false, "capIsNewExceptions": ["Router"] }], 
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }] 
    } 
} 